<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:local="clr-namespace:CurlEz.Views"
       x:Class="CurlEz.AppShell">

    <ShellContent Title="Main Page" ContentTemplate="{DataTemplate local:MainPage}" />
    <ShellContent Title="Second Page" ContentTemplate="{DataTemplate local:SecondPage}" Route="SecondPage" />
    <ShellContent Title="Test Page" ContentTemplate="{DataTemplate local:TestView}" Route="TestView" />
</Shell>
